@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">User Information</h1>
    <p>FirstName: @Html.Encode(Model.FirstName)</p>
    <p>LastName: @Html.Encode(Model.LastName)</p>
    <p>Gender: @Html.Encode(Model.Gender)</p>
    <p>Email Address: @Html.Encode(Model.EmailAddress)</p>
    <p>NRIC: @Model.NRIC</p>
    <p>Who Am I: @Model.WhoAmI</p>
</div>
